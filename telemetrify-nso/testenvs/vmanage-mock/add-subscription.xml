<?xml version="1.0" encoding="utf-8"?>
<config xmlns="http://tail-f.com/ns/config/1.0">
  <telemetrify xmlns="urn:telekom:abs:telemetrify">
    <subscription>
      <name>sub1</name>
      <source>
        <source-ref>vmanage</source-ref>
        <vmanage-poll>
          <path>/statistics/approute</path>
          <period>5000</period>
        </vmanage-poll>
        <schema>
          <ned-id xmlns:ned-vmanage-yang-nc-1.0="http://tail-f.com/ns/ned-id/ned-vmanage-yang-nc-1.0">ned-vmanage-yang-nc-1.0:ned-vmanage-yang-nc-1.0</ned-id>
        </schema>
      </source>
      <output>
        <transform>
          <transform-ref>vmanage</transform-ref>
        </transform>
        <!-- <sink>
          <sink-ref>m3db</sink-ref>
        </sink> -->
      </output>
    </subscription>
    <source>
      <name>vmanage</name>
      <mock-vmanage-http/>
      <!-- <vmanage-http>
            <address>vmanage-netsim1</address>
            <port>443</port>
            <username>vrnetlab</username>
            <password>VR-netlab9</password>
      </vmanage-http> -->
    </source>
    <sink>
      <name>cdb</name>
      <nso-cdb>
      </nso-cdb>
    </sink>
    <!-- <sink>
      <name>m3db</name>
      <m3db>
        <address>m3db</address>
        <port>7201</port>
      </m3db>
    </sink> -->
    <transform>
      <name>vmanage</name>
      <type>
        <vmanage xmlns="urn:telekom:abs:telemetrify-transform"/>
      </type>
      <output>
        <sink>
          <sink-ref>cdb</sink-ref>
        </sink>
      </output>
    </transform>
    <logging>
      <!-- <level>debug</level> -->
      <level>trace</level>
    </logging>
  </telemetrify>
</config>
